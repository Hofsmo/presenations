	shortjournal = {Renewable and Sustainable Energy Reviews},
	author = {Kishor, Nand and Saini, R. P. and Singh, S. P.},
	urldate = {2016-08-30},
	date = {2007-06},
	keywords = {Control approach, Hydroturbine, modeling, Nonlinear},
	file = {ScienceDirect Full Text PDF:/home/sigurd/.zotero/zotero/h97xxbq0.default/zotero/storage/5DK2XDBJ/Kishor et al. - 2007 - A review on hydropower plant models and control.pdf:application/pdf;ScienceDirect Snapshot:/home/sigurd/.zotero/zotero/h97xxbq0.default/zotero/storage/PT9GFJZ8/S136403210500064X.html:text/html}
