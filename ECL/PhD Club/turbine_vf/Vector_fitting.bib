	keywords = {Circuit simulation, circuit simulator, circuit solver integration method, Convolution, Fitting, Frequency dependence, Frequency domain analysis, integration, Linear macromodels, numerical integration method, power system analysis computing, Power system modeling, rational modeling, Robustness, simulated time domain responses, {TD}-{VF}, time-domain analysis, time domain vector fitting, Transfer functions, vectors, Z-domain vector fitting, {ZD}-{VF}},
	file = {IEEE Xplore Abstract Record:/home/sigurd/.zotero/zotero/h97xxbq0.default/zotero/storage/UGXVEBBB/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/sigurd/.zotero/zotero/h97xxbq0.default/zotero/storage/G3FVTQJ7/Ubolli og Gustavsen - 2010 - Applicability of time domain and z-domain Vector F.pdf:application/pdf}
