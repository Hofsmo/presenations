@article{gustavsen_rational_1999,
	title = {Rational approximation of frequency domain responses by vector fitting},
	volume = {14},
	issn = {0885-8977},
	doi = {10.1109/61.772353},
	abstract = {The paper describes a general methodology for the fitting of measured or calculated frequency domain responses with rational function approximations. This is achieved by replacing a set of starting poles with an improved set of poles via a scaling procedure. A previous paper (Gustavsen et al., 1997) described the application of the method to smooth functions using real starting poles. This paper extends the method to functions with a high number of resonance peaks by allowing complex starting poles. Fundamental properties of the method are discussed and details of its practical implementation are described. The method is demonstrated to be very suitable for fitting network equivalents and transformer responses. The computer code is in the public domain, available from the first author},
	pages = {1052--1061},
	number = {3},
	journaltitle = {{IEEE} Transactions on Power Delivery},
	author = {Gustavsen, B. and Semlyen, A.},
	date = {1999-07},
	keywords = {Computational modeling, computer simulation, Convolutional codes, Frequency dependence, Frequency domain analysis, frequency-domain analysis, frequency domain responses, Frequency measurement, function approximation, poles and zeros, power network equivalents, power system analysis computing, power system dynamics, Power system modeling, power system simulation, Power system transients, power system transients modelling, rational approximation, Resonance, resonance peaks, scaling procedure, smooth functions, starting poles, transformer responses, vector fitting, vectors},
	file = {IEEE Xplore Abstract Record:/home/sigurd/.zotero/zotero/h97xxbq0.default/zotero/storage/CJKM5AVF/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/sigurd/.zotero/zotero/h97xxbq0.default/zotero/storage/E37XC6SE/Gustavsen og Semlyen - 1999 - Rational approximation of frequency domain respons.pdf:application/pdf}
}

@article{ramirez_vector_2009,
	title = {Vector Fitting-Based Calculation of Frequency-Dependent Network Equivalents by Frequency Partitioning and Model-Order Reduction},
	volume = {24},
	issn = {0885-8977},
	doi = {10.1109/TPWRD.2008.921139},
	abstract = {This paper presents a methodology to identify a frequency-dependent network equivalent from a highly resonant frequency response and in a broad range of frequencies. Primarily, the identification process consists on dividing the complete frequency range into a number of subranges either uniformly spaced or by containing an equal number of resonance peaks. Vector fitting ({VF}) is then applied to each subrange, yielding an accurate local approximation. A refinement of the local set of partial fractions is made through model-order reduction to remove spurious (redundant) poles. The final equivalent comprises a reduced number of poles, keeping the computational load lower than when applying {VF} to the whole range of frequencies while giving a similar rms error. An illustrative example involving a transmission system is presented for validation of the procedures above.},
	pages = {410--415},
	number = {1},
	journaltitle = {{IEEE} Transactions on Power Delivery},
	author = {Ramirez, A.},
	date = {2009-01},
	keywords = {Circuit simulation, Circuit synthesis, Computational modeling, equivalent circuits, Frequency conversion, frequency-dependent network equivalents, frequency-domain synthesis, frequency partitioning, highly resonant frequency response, Least squares approximation, mean square error methods, model-order reduction, model reduction, Network synthesis, poles, rational approximation, reduced order systems, Resonance, Resonant frequency, rms error, transmission networks, transmission system, vector fitting}
}
