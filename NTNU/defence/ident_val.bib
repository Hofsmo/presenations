@article{hannett_field_1994,
	title = {Field tests to validate hydro turbine-governor model structure and parameters},
	volume = {9},
	issn = {0885-8950},
	doi = {10.1109/59.331426},
	abstract = {A test procedure was developed to obtain accurate governor models for hydroelectric stations owned by the New York Power Authority to improve the accuracy of stability simulations of the New York State system. Tests were performed on units at three stations and values for parameters of previously available model structures were derived from the information obtained during these tests. However, it was found that the model structures were not adequate to represent the entire range of operating conditions. Additional examination of test measurements and model structures indicated that the model structure needed to be refined. This paper reports on the refinements made to the hydro turbine-governor models which enabled reproduction of the physical system responses. Simulation results indicate that the refined models with the corresponding identified parameters produce responses which closely fit those recorded from the tests performed on three hydro units},
